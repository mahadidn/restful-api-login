{
    "openapi": "3.0.2",
    "info": {
        "title": "User API",
        "description": "User API",
        "version": "1.0"
    },
    "servers": [{ "url": "https://localhost:8000" }],
    "paths": {
        "/api/login": {
            "post": {
                "description": "Login User",
                "requestBody": {
                    "content": {
                        "application/json": {
                            "schema": {
                                "type": "object",
                                "properties": {
                                    "email": {
                                        "type": "string"
                                    },
                                    "password": {
                                        "type": "string"
                                    }
                                }
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "Success login",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "object",
                                    "properties": {
                                        "message": {
                                            "type": "string"
                                        },
                                        "access_token": {
                                            "type": "string"
                                        },
                                        "token_type": {
                                            "type": "string"
                                        }
                                    }
                                }
                            }
                        }
                    },
                    "401": {
                        "description": "Failed Login",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "object",
                                    "properties": {
                                        "message": {
                                            "type": "string"
                                        }
                                    }
                                }
                            }
                        }
                    }
                }
            }
        },
        "/api/register": {
            "post": {
                "description": "Register User",
                "requestBody": {
                    "content": {
                        "application/json": {
                            "examples": {
                                "mahadi": {
                                    "description": "Register User Mahadi",
                                    "value": {
                                        "name": "Mahadi Dwi Nugraha",
                                        "email": "mahadi@example.com",
                                        "password": "mahadi123"
                                    }
                                }
                            },
                            "schema": {
                                "type": "object",
                                "required": ["name", "email", "password"],
                                "properties": {
                                    "name": {
                                        "type": "string"
                                    },
                                    "email": {
                                        "type": "string"
                                    },
                                    "password": {
                                        "type": "string"
                                    }
                                }
                            }
                        }
                    }
                },
                "responses": {
                    "201": {
                        "description": "Success Register User",
                        "content": {
                            "application/json": {
                                "examples": {
                                    "success": {
                                        "description": "Success Register New User",
                                        "value": {
                                            "data": {
                                                "name": "Mahadi Dwi Nugraha",
                                                "email": "mahadidwinugraha@gmail.com",
                                                "updated_at": "2024-07-03T04:43:26.000000Z",
                                                "created_at": "2024-07-03T04:43:26.000000Z",
                                                "id": 1
                                            },
                                            "access_token": "5|TmFgOo3TEJpETTjud9tCaVMSK5TY20J1iyb0Pn9o62b9d200",
                                            "token_type": "Bearer"
                                        }
                                    }
                                },
                                "schema": {
                                    "type": "object",
                                    "properties": {
                                        "data": {
                                            "type": "object",
                                            "properties": {
                                                "id": {
                                                    "type": "number"
                                                },
                                                "name": {
                                                    "type": "string"
                                                },
                                                "email": {
                                                    "type": "string"
                                                }
                                            }
                                        },
                                        "errors": {
                                            "type": "object"
                                        }
                                    }
                                }
                            }
                        }
                    },
                    "400": {
                        "description": "Validation Error",
                        "content": {
                            "application/json": {
                                "examples": {
                                    "Validation Error": {
                                        "description": "Validation Error",
                                        "value": {
                                            "email": [
                                                "The email field is required",
                                                "email containt @examples.com",
                                                "The email has already been taken"
                                            ],
                                            "password": [
                                                "The password field is required",
                                                "password min 8 characters"
                                            ],
                                            "name": [
                                                "The name field is required"
                                            ]
                                        }
                                    }
                                }
                            }
                        }
                    }
                }
            }
        },
        "/api/user": {
            "get": {
                "description": "Get User Data",
                "parameters": [
                    {
                        "name": "Authorization",
                        "in": "header"
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Success get current user",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "object",
                                    "properties": {
                                        "id": {
                                            "type": "number"
                                        },
                                        "name": {
                                            "type": "string"
                                        },
                                        "email": {
                                            "type": "string"
                                        },
                                        "email_verified_at": {
                                            "type": "string"
                                        },
                                        "created_at": {
                                            "type": "string"
                                        },
                                        "updated_at": {
                                            "type": "string"
                                        }
                                    }
                                }
                            }
                        }
                    }
                }
            }
        },
        "/api/logout":{
            "delete":{
                "description": "Logout current user",
                "parameters": [
                    {
                        "name": "Authorization",
                        "in": "header"
                    }
                ],
                "responses": {
                    "200":{
                        "description": "Success Logout User",
                        "content": {
                            "application/json":{
                                "schema":{
                                    "type": "object",
                                    "properties": {
                                        "message":{
                                            "type": "string"
                                        }
                                    }
                                }
                            }
                        }
                    }
                }
            }
        }
    }
}
